{
  "name": "primetracking-server",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=14.15.1"
  },
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "docker-compose build",
    "dev:clean": "docker-compose down && docker image prune -f && docker container prune -f && docker volume prune -f",
    "start:dev": "NODE_ENV=development nodemon -L --watch app --exec 'node ./server.js'",
    "start:prod": "NODE_ENV=production node ./server.js",
    "docker": "npm run build && npm run docker:mongo && npm run docker:server",
    "docker:server": "docker-compose up server",
    "docker:mongo": "docker-compose up -d mongo"
  },
  "author": "Damanveer Singh",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.18.3",
    "colors": "1.3.3",
    "connect-mongodb-session": "2.2.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.11.0",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "helmet": "^3.21.2",
    "mongoose": "5.11.1",
    "mongoose-paginate": "5.0.3",
    "morgan": "1.9.1",
    "pug": "^3.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-plugin-node": "^8.0.0",
    "nodemon": "^1.19.4"
  }
}
